<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Background colors-->
    <SolidColorBrush x:Key="BGColor1" Color="#252836"/>
    <SolidColorBrush x:Key="BGColor2" Color="#1F1D2B"/>
    <SolidColorBrush x:Key="BGColor3" Color="#2D303E"/>
    
    <!--Border colors-->
    <SolidColorBrush x:Key="borderColor1" Color="#FF7A00"/>
    <SolidColorBrush x:Key="borderColor2" Color="#A9A9A9"/>
    
    <!--Text-->
    <SolidColorBrush x:Key="titleColor" Color="#FF7A00"/>
    <SolidColorBrush x:Key="textColor1" Color="#FFF"/>
    <SolidColorBrush x:Key="textColor2" Color="#A9A9A9"/>
    <SolidColorBrush x:Key="textColor-error" Color="#D75960"/>
    
    <!--Button-->
    <SolidColorBrush x:Key="buttonColor1" Color="#FF7A00"/>

    <SolidColorBrush x:Key="SelectedBackground" Color="#ED7D31" />
    <SolidColorBrush x:Key="SideBarbackground" Color="#4F4A45" />
    <SolidColorBrush x:Key="Contentbackground" Color="#6C5F5B" />
    <SolidColorBrush x:Key="TextColor" Color="#F6F1EE" />
    <SolidColorBrush x:Key="SelectedTextColor" Color="#ED7D31" />
</ResourceDictionary>